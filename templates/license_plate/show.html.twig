{% extends 'base.html.twig' %}

{% block title %}Show licenseplate{% endblock %}

{% block body %}
    <h1>Show licenseplate</h1>
    <div class="container">
        <table class="table">
            <tbody>
            <tr>
                <th>Id</th>
                <td>{{ license_plate.id }}</td>
            </tr>
            <tr>
                <th>License_plate</th>
                <td>{{ license_plate.licensePlate }}</td>
            </tr>
            </tbody>
        </table>
        <div class="row mx-5">
            <a class="btn btn-primary btn-lg mt-3" href="{{ path('license_plate_edit', {'id': license_plate.id}) }}">edit</a>
        </div>
        <div class="row mx-5">
            <a class="btn btn-primary btn-lg mt-3" href="{{ path('license_plate_delete', {'id': license_plate.id}) }}">delete</a>
        </div>
        <div class="row mx-5">
            <a class="btn btn-primary btn-lg mt-3" href="{{ path('license_plate_index') }}">Back to my cars</a>
        </div>

    </div>
{% endblock %}
